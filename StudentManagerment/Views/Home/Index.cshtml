@using StudentManagerment.Models
@model StudentManagerment.ViewModels.HomeDetailsViewModel
@{
    Layout = @"~/Views/Shared/_Layout.cshtml";
    ViewBag.Title =Model.PageTitle;
}

<table border="1">
    <tr>
        <td>id</td>
        <td>姓名</td>
        <td>班级</td>
        <td>邮箱</td>
    </tr>
    @foreach (var item in Model.stuArr)
    {
        <tr>
            <td>@item.id</td>
            <td>@item.Name</td>
            <td>@item.ClassName</td>
            <td>@item.Email</td>
        </tr>
    }
</table>
